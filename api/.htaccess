
#RewriteCond %{REQUEST_FILENAME} -d [OR]
RewriteCond %{REQUEST_FILENAME} -f
RewriteRule (.*) - [S=3]
RewriteRule ^(robots|mechas|bosses|fields|abilities|items|types)/index/all/?\??(.*)$ database-v2/$1/index.php?include_all=true&$2 [L]
RewriteRule ^(robots|mechas|bosses|fields|abilities|items|types)/index/?\??(.*)$ database-v2/$1/index.php?$2 [L]
RewriteRule ^(robots|mechas|bosses|fields|abilities|items|types)/all/?\??(.*)$ database-v2/$1/tokens.php?include_all=true&$2 [L]
RewriteRule ^(robots|mechas|bosses|fields|abilities|items|types)/([-_a-z0-9]+)/?\??(.*)$ database-v2/$1/data.php?token=$2&$3 [L]
RewriteRule ^(robots|mechas|bosses|fields|abilities|items|types)/?\??(.*)$ database-v2/$1/tokens.php?$2 [L]