
#RewriteCond %{REQUEST_FILENAME} -d [OR]
RewriteCond %{REQUEST_FILENAME} -f
RewriteRule (.*) - [S=3]
RewriteRule ^(robots|mechas|bosses|fields|abilities|items|types)/index/all/?\??(.*)$ index.php?kind=$1&script=index&include_all=true&$2 [L]
RewriteRule ^(robots|mechas|bosses|fields|abilities|items|types)/index/?\??(.*)$ index.php?kind=$1&script=index&$2 [L]
RewriteRule ^(robots|mechas|bosses|fields|abilities|items|types)/all/?\??(.*)$ index.php?kind=$1&script=tokens&include_all=true&$2 [L]
RewriteRule ^(robots|mechas|bosses|fields|abilities|items|types)/([-_a-z0-9]+)/?\??(.*)$ index.php?kind=$1&script=data&token=$2&$3 [L]
RewriteRule ^(robots|mechas|bosses|fields|abilities|items|types)/?\??(.*)$ index.php?kind=$1&script=tokens&$2 [L]